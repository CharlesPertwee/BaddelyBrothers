<odoo>
	<data>
		<record id="enquiry_conversion_tree_view" model="ir.ui.view">
	        <field name="name">enquiry_conversion_tree</field>
	        <field name="model">bb_estimate.lead_conversion_report</field>
	        <field name="arch" type="xml">
				<tree string="Conversion">
	                <field name="FinancialYear" />
	                <field name="Year" />
	                <field name="Month" />
	                <field name="EnquiryType" />
	                <field name="TotalEnquiry" />
	                <field name="ConvertedEnquriy" />
	                <field name="ConvertedOrder" />
	            </tree>
	        </field>
	    </record>

	    <record id="bb_estimate_enquiry_conversion" model="ir.actions.act_window">
	        <field name="name">Enquiry Conversion</field>
	        <field name="res_model">bb_estimate.lead_conversion_report</field>
	        <field name="view_mode">tree</field>
		</record> 

	    <menuitem id="bb_conversion_analysis_report" name="Conversion Analysis" parent="crm.crm_menu_report" action="bb_estimate_enquiry_conversion"/>
	</data>
</odoo>