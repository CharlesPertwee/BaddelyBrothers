<odoo>
	<data>
		<record id="ticket_times_tree_view" model="ir.ui.view">
	        <field name="name">ticket_times_tree</field>
	        <field name="model">bb_estimate.ticket_times_report</field>
	        <field name="arch" type="xml">
				<tree string="Conversion">
					<field name="FinancialYear" />
					<field name="Year" />
	                <field name="Month" />
	                <field name="OrderType" />
	                <field name="AvgConversionTime" />
	            </tree>
	        </field>
	    </record>

	    <record id="ticket_times" model="ir.actions.act_window">
	        <field name="name">Ticket Times Report</field>
	        <field name="res_model">bb_estimate.ticket_times_report</field>
	        <field name="view_mode">tree</field>
		</record> 

	    <menuitem id="ticket_times_report" name="Conversion Times" parent="sale.menu_sale_report" action="ticket_times"/>
	</data>
</odoo>