<odoo>
	<data>
		<record id="estimate_rates_tree_view" model="ir.ui.view">
	        <field name="name">estimate_times_tree</field>
	        <field name="model">bb_estimate.estimate_rates_months</field>
	        <field name="arch" type="xml">
				<tree string="Conversion">
	                <field name="Year" />
	                <field name="Month" />
	                <field name="TotalEstimates" />
	                <field name="ConvertedOrder" />
	                 <field name="ConvertedOrderPercent" />
	            </tree>
	        </field>
	    </record>

	    <record id="estimate_rates_conversion" model="ir.actions.act_window">
	        <field name="name">Estimate Conversions</field>
	        <field name="res_model">bb_estimate.estimate_rates_months</field>
	        <field name="view_mode">tree</field>
		</record> 

	    <menuitem id="estimate_rates_report" name="Conversion Rates my Month" parent="bb_estimate.bb_estimate_reports" action="estimate_rates_conversion"/>
	</data>
</odoo>