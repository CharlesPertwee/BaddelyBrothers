<odoo>
    <data>
        <record id="deleted_line_info" model="ir.ui.view">
            <field name="name">Deleted Line Info</field>
            <field name="model">estimate.delete_line_info</field>
            <field name="arch" type="xml">
                <form string="Changes in Material Quantities">
                    <sheet>
                        <group>
                            <field name="info" readonly="1" nolabel="1"/>
                        </group>
                        <footer>
                            <button name="write" type="object" string="Ok"/>
                            <button string="Cancel" class="oe_right" special="cancel"/>
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>