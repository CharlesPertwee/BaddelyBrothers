<odoo>
    <data>
        <record id="estimate_line_tree_view" model="ir.ui.view">
            <field name="name">estimate.line.tree</field>
            <field name="model">bb_estimate.estimate_line</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="lineName"/>
                    <field name="customer_description"/>
                    <field name="total_price_1" sum="Total Price 1"/>
                    <field name="total_price_2" sum="Total Price 2"/>
                    <field name="total_price_3" sum="Total Price 3"/>
                    <field name="total_price_4" sum="Total Price 4"/>
                    <field name="total_price_run_on" sum="Total Run On"/>
                </tree>
            </field>
        </record>
        
        <record id="estimate_line_form_view" model="ir.ui.view">
            <field name="name">estimate.line.form</field>
            <field name="model">bb_estimate.estimate_line</field>
            <field name="arch" type="xml">
                <form string="Estimate Line">
                    <sheet>
                        <field name="estimate_id" invisible="1" />
                        <field name="req_param_number_up" invisible="1" />
                        <field name="req_param_number_out" invisible="1" />
                        <field name="req_param_number_of_colors" invisible="1" />
                        <field name="req_param_number_of_colors_rev" invisible="1" />
                        <field name="req_work_twist" invisible="1" />
                        <field name="req_grammage" invisible="1" />
                        <field name="req_param_no_of_ink_mixes" invisible="1" />
                        <field name="req_param_additional_charge" invisible="1" />
                        <field name="req_param_misc_charge_per_cm2" invisible="1" />
                        <field name="req_param_die_size" invisible="1" />
                        <field name="req_param_printed_material" invisible="1"/>
                        <field name="req_param_duplex_sheets" invisible="1" />
                        <field name="req_param_number_of_sheets" invisible="1" />
                        <field name="req_param_number_of_cuts" invisible="1" />
                        <field name="req_param_sheets_per_pile" invisible="1" />
                        <field name="req_param_time_per_pile" invisible="1" />
                        <field name="req_param_env_windowpatching" invisible="1" />
                        <field name="req_param_env_peelandstick" invisible="1" />
                        <field name="req_param_env_inlineemboss" invisible="1" />
                        <field name="req_param_env_gumming" invisible="1" />
                        <field name="req_param_material_line_id" invisible="1" />
                        
                        <!--div class="oe-title">
                                            <field name="option_type" options="{'horizontal': true}" widget="radio"/>
                                        </div-->
                        <div class="container">
                            <div class="row">
                                <div class="col-md-4 oe-title pull-left">
                                    <field name="option_type" options="{'horizontal': true}" widget="radio" />
                                </div>
                                <div class="col-md-8 pull-right" attrs="{'invisible':[('option_type','!=','process')]}">
                                    <div class="form-group">
                                        <label for="workcenterId" class="col-md-4" />
                                        <field name="workcenterId" class="col-md-7" />
                                    </div>
                                </div>
                                <div class="col-md-8 pull-right" attrs="{'invisible':[('option_type','!=','material')]}">
                                    <div class="form-group">
                                        <label for="material" class="col-md-4" />
                                        <field name="material" class="col-md-7" />
                                    </div>
                                </div>
                            </div>
                            <notebook colspan="4">
                                <page string="Description">
                                    <div class="row">
                                        <div class="col">
                                            <div class="col-md-6">
                                                <label for="customer_description" class="col-md-6" />
                                                <field name="customer_description" class="col-md-6" />
                                            </div>
                                        </div>
                                    </div>
                                </page>
                                <page string="Process" attrs="{'invisible':['|',('option_type','=','process'),('option_type','=',False)]}">
                                </page>
                                <page string="Materials" attrs="{'invisible':['|',('option_type','=','material'),('option_type','=',False)]}">
                                </page>
                                <page string="Parameters &amp; Costing" autofocus="autofocus">
                                    <p style="font-size:10px;">General Parameters</p>
                                    <hr />
                                    <div class="row" style="font-size:9.80px;">
                                        <div class="col" attrs="{'invisible':['&amp;',('material','=',False),('workcenterId','=',False)]}">
                                            <div class="row">
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_printed_material','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_printed_material" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_printed_material" class="col-md-6"/>
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_number_of_cuts','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_number_of_cuts" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_number_of_cuts" class="col-md-6" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_sheets_per_pile','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_sheets_per_pile" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_sheets_per_pile" class="col-md-6" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_time_per_pile','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_time_per_pile" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_time_per_pile" class="col-md-6" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('option_type','!=','material'),('req_param_number_out','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_number_out" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_number_out" class="col-md-6" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_number_up','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_number_up" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_number_up" class="col-md-6" readonly="1" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_work_twist','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="work_twist" class="col-md-5" style="font-size:10px;" />
                                                        <field name="work_twist" class="col-md-6" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row" >
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_number_of_colors','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_number_of_colors" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_number_of_colors" class="col-md-6" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_grammage','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="grammage" class="col-md-5" style="font-size:10px;" />
                                                        <field name="grammage" class="col-md-6" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_duplex_sheets','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_duplex_sheets" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_duplex_sheets" class="col-md-6" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_number_of_sheets','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_number_of_sheets" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_number_of_sheets" class="col-md-6" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_additional_charge','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_additional_charge" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_additional_charge" class="col-md-6" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_material_line_id','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_material_line_id" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_material_line_id" class="col-md-6" create_edit="false" domain="[('option_type','=','material'),('estimate_id','=',estimate_id)]"/>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_env_windowpatching','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_env_windowpatching" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_env_windowpatching" class="col-md-6" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_env_peelandstick','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_env_peelandstick" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_env_peelandstick" class="col-md-6" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_env_inlineemboss','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_env_inlineemboss" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_env_inlineemboss" class="col-md-6" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_env_gumming','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_env_gumming" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_env_gumming" class="col-md-6" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_number_of_colors_rev','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_number_of_colors_rev" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_number_of_colors_rev" class="col-md-6" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_no_of_ink_mixes','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_no_of_ink_mixes" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_no_of_ink_mixes" class="col-md-6" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4" attrs="{'invisible':[('req_param_die_size','=',False)]}">
                                                    <div class="form-group">
                                                        <label for="param_die_size" class="col-md-5" style="font-size:10px;" />
                                                        <field name="param_die_size" class="col-md-6" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row" style="font-size:10px;font-weight:bold;">
                                            <div class="col">
                                                <div class="row">
                                                    <div class="col-md-2">
                                                        <p>Parameter</p>
                                                        <hr />
                                                    </div>
                                                    <div class="col-md-2">
                                                        <p>Qty 1</p>
                                                        <hr />
                                                    </div>
                                                    <div class="col-md-2">
                                                        <p>Qty 2</p>
                                                        <hr />
                                                    </div>
                                                    <div class="col-md-2">
                                                        <p>Qty 3</p>
                                                        <hr />
                                                    </div>
                                                    <div class="col-md-2">
                                                        <p>Qty 4</p>
                                                        <hr />
                                                    </div>
                                                    <div class="col-md-2">
                                                        <p>Run on</p>
                                                        <hr />
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-2 form-group">
                                                       <span class="o_field_char o_field_widget o_readonly_modifier"></span>
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="quantity_1" readonly="1" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="quantity_2" readonly="1" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="quantity_3" readonly="1" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="quantity_4" readonly="1" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="run_on" readonly="1" />
                                                    </div>
                                                </div>
                                                <div class="row" attrs="{'invisible':[('option_type','=','material')]}">
                                                    <div class="col-md-2 form-group">
                                                        <span class="o_field_char o_field_widget o_readonly_modifier">Make Ready Time(Hours)</span>
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_make_ready_time_1" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_make_ready_time_2" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_make_ready_time_3" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_make_ready_time_4" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_make_ready_time_run_on" />
                                                    </div>
                                                </div>
                                                <div class="row" attrs="{'invisible':[('option_type','=','material')]}">
                                                    <div class="col-md-2 form-group">
                                                        <span class="o_field_char o_field_widget o_readonly_modifier">Speed(sheets per Hour)</span>
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_machine_speed_1" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_machine_speed_2" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_machine_speed_3" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_machine_speed_4" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_machine_speed_run_on" />
                                                    </div>
                                                </div>
                                                <div class="row" attrs="{'invisible':[('option_type','=','material')]}">
                                                    <div class="col-md-2 form-group">
                                                        <span class="o_field_char o_field_widget o_readonly_modifier">Running Time(Hours)</span>
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_running_time_1" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_running_time_2" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_running_time_3" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_running_time_4" />
                                                    </div>
                                                    <div class="col-md-2 form-group">
                                                        <field name="param_running_time_run_on" />
                                                    </div>
                                                </div>
                                                <div class="row" attrs="{'invisible':[('option_type','=','material')]}">
