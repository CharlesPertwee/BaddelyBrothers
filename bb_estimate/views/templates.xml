                                    <option t-attf-value="gummed">Gummed</option>
                                    <option t-attf-value="doublegummed">Double Gummed</option>
                                    <option t-attf-value="peel">Peel &amp; Stick</option>
                                    <option t-attf-value="ungummed">Un-gummed</option>
                                    <option t-attf-value="topless">Topless</option>
                                    <option t-attf-value="stringwasher">String &amp; Washer</option>
                                 </select>
                            </div>
                        </div>
                        <div class="envelope form-group row form-field">
                            <label class="col-lg-3 col-md-4 col-form-label" for="enquiryEnvelopeTissuelIne">Tissue Lined</label>
                            <div class="col-lg-7 col-md-8">
                                <select class="form-control" name="enquiryEnvelopeTissuelIne">
                                    <option t-attf-value="unlined">Unlined</option>
                                    <option t-attf-value="full">Yes - Fully</option>
                                    <option t-attf-value="half">Yes - Half</option>
                                 </select>
                            </div>
                            <label class="col-lg-3 col-md-4 col-form-label"></label>
                            <div class="col-lg-7 col-md-8">
                                <p style="font-size:10px;">Your envelopes can be made unlined, fully-lined with tissue paper. We have a range of colors to choose from</p>
                            </div>
                        </div>
                       <div class="form-group row form-field">
                            <label class="col-lg-3 col-md-4 col-form-label" for="enquiryMaterial">Material</label>
                            <div class="col-lg-7 col-md-8">
                                <input type="text" class="form-control o_website_form_input" name="enquiryMaterial" t-att-value="request.params.get('enquiryMaterial', '')"/>
                            </div>
                            <label class="col-lg-3 col-md-4 col-form-label"></label>
                            <div class="col-lg-7 col-md-8">
                                <p style="font-size:10px;">Do you have a paper/board preference: brand,colour, thickness</p>
                            </div>
                        </div> 
                        <div class="form-group row form-field">
                            <label class="col-lg-3 col-md-4 col-form-label" for="enquiryWeightGsm">Weight(g.s.m)</label>
                            <div class="col-lg-7 col-md-8">
                                <input type="number" class="form-control o_website_form_input" name="enquiryWeightGsm" t-att-value="request.params.get('enquiryWeightGsm', '')"/>
                            </div>
                        </div>
                        <div class="form-group row form-field">
                            <label class="col-lg-3 col-md-4 col-form-label" for="description">Comments</label>
                            <div class="col-lg-7 col-md-8">
                                <textarea type="text" class="form-control o_website_form_input" name="description" t-att-value="request.params.get('description', '')"/>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="offset-lg-3 offset-md-4 col-md-8 col-lg-7">
                                <a href="#" role="button" class="btn btn-primary btn-lg o_website_form_send">Send</a>
                                <span id="o_website_form_result"></span>
                            </div>
                        </div>
                    </form>
                </div>

            </xpath>
        </template>
    </data>
</odoo>