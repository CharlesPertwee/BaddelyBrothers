<odoo>
    <data>
        <template id="assets_backend_contact_form" name="static_resources_demo assets" inherit_id="web.assets_frontend">
            <xpath expr="." position="inside">
                <!--These links will be called when loading your Odoo -->
                <script type="text/javascript" src="/bb_estimate/static/src/js/ContactForm.js"></script>
            </xpath>
        </template>
        
        <template id="assets_backend_delete_confirmation" name="Delete Confirmation" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="application/javascript" src="/bb_estimate/static/src/js/DeleteConfirmation.js"/>
            </xpath>
        </template>
        
        <template id="assets_backend_estimate_css" name="bb_estimate_style assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <!--These links will be called when loading your Odoo -->
                <link rel="stylesheet" href="/bb_estimate/static/src/css/Estimate.css"/>
            </xpath>
        </template>
        
        <template id="assets_backend_estimate__font_css" name="BB Georgia Font" inherit_id="web.assets_common">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" href="/bb_estimate/static/src/fonts/Georgia/style.css"/>
            </xpath>
        </template>
        
        <template id="change_log">
            <div class="col-sm-12">
                <table class="table text-center table-bordered">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th colspan="2">Qty 1</th>
                            <th colspan="2">Qty 2</th>
                            <th colspan="2">Qty 3</th>
                            <th colspan="2">Qty 4</th>
                            <th colspan="2">Run On</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td></td>
                            <td>Old</td>
                            <td>New</td>
                            <td>Old</td>
                            <td>New</td>
                            <td>Old</td>
                            <td>New</td>
                            <td>Old</td>
                            <td>New</td>
                            <td>Old</td>
                            <td>New</td>
                        </tr>
                        <tr t-foreach="lines" t-as="line">
                            <td><t t-esc="line[0]"/></td>
                            <td><t t-esc="line[1]"/></td>
                            <td><t t-esc="line[2]"/></td>
                            <td><t t-esc="line[3]"/></td>
                            <td><t t-esc="line[4]"/></td>
                            <td><t t-esc="line[5]"/></td>
                            <td><t t-esc="line[6]"/></td>
                            <td><t t-esc="line[7]"/></td>
                            <td><t t-esc="line[8]"/></td>
                            <td><t t-esc="line[9]"/></td>
                            <td><t t-esc="line[10]"/></td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </template>
        
        <template id="qty_amend">
            <div class="alert alert-warning" role="alert"> 
                Quantity Ammended
                <div class="mt16">
                    <p>Old Qty: <t t-esc="data.Quantity + data.RunOn" /> | Old Price: <t t-esc="data.TotalPrice" /></p>
                    <p>New Qty: <t t-esc="data.AmmendedQty + data.RunOn" /> | Old Price: <t t-esc="data.AmmendedPrice" /></p>
                    <p>Changes(s):</p>
                    <ul t-foreach="data.ChangeLog" t-as="line">
                        <li><t t-if="line.product_id">
                                <t t-esc="line.product_id.name" />
                            </t>
                            <t t-else="">
                                <t t-esc="line.workcenter_id.name" />
                            </t>
                            Old: 
                            <t t-esc="'%0.2f'%(line.CurrentRequired)" />
                            New: 
                            <t t-esc="'%0.2f'%(line.NewRequired)" />
                            PO: 
                            <t t-esc="'%0.2f'%(line.POQty)" />
                            Comment: 
                            <t t-esc="line.POComment" />
                          </li>
                    </ul>
                </div>
            </div>
        </template>
        
        <template id="bb_contactus_form" name="BB Contact Form" inherit_id="website_crm.contactus_form" customize_show="True">
            <xpath expr="//*[@id='wrap']/div[2]/div/div[1]/div[2]/form" position="replace">
                <div style="background: #F3F3F3;padding: 20px;">
                    <form action="/website_form/" method="post" data-model_name="crm.lead" data-success_page="/contactus-thank-you" class="s_website_form container-fluid mt32" enctype="multipart/form-data" data-editable-form="false">
