<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="required" model="bb_estimate.stage">
            <field name="name">Estimate Required</field>
        </record>

        <record id="in_progress" model="bb_estimate.stage">
            <field name="name">Estimate In Progress</field>
        </record>

        <record id="provided" model="bb_estimate.stage">
            <field name="name">Estimate Provided</field>
            <field name="isOrder" eval="True"/>
        </record>

        <record id="converted" model="bb_estimate.stage">
            <field name="name">Estimate Converted</field>
            <field name="ConvertedStage" eval="True"></field>
        </record>

        <record id="declined" model="bb_estimate.stage">
            <field name="name">Estimate Declined</field>
        </record>
    </data>
</odoo> 