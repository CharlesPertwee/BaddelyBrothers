<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_bb_estimate" model="ir.sequence">
            <field name="name">Job Estimate</field>
            <field name="code">bb_estimate.estimate</field>
            <field name="prefix">EST/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <record id="sequence_bb_jobticket" model="ir.sequence">
            <field name="name">Job Ticket</field>
            <field name="code">bb_estimate.jobticket</field>
            <field name="prefix">BB6</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <record forcecreate="True" id="estimate_accuracy" model="decimal.precision">
            <field name="name">Estimate</field>
            <field name="digits">2</field>
        </record>
        
    </data>
</odoo> 