<odoo>
    <!-- For Purchase Order Email Template -->
    <function name="write" model="ir.model.data">
         <!-- get the record if from data base -->
        <function name="search" model="ir.model.data">
            <value
              eval="[('module', '=', 'purchase'), ('name', '=', 'email_template_edi_purchase_done')]"
              />
        </function>
       <!-- remove noupdate -->
        <value eval="{'noupdate': False}" />
    </function>
    
    <record id="purchase.email_template_edi_purchase_done" model="mail.template">
        <field name="report_template" ref="purchase_order_report"/>
    </record>
    
    <function name="write" model="ir.model.data">
        <function name="search" model="ir.model.data">
            <value
              eval="[('module', '=', 'purchase'), ('name', '=', 'email_template_edi_purchase_done')]"
              />
        </function>
        <value eval="{'noupdate': True}" />
    </function>
    
    <!-- For Request For Quotation Email Template -->
    <function name="write" model="ir.model.data">
         <!-- get the record if from data base -->
        <function name="search" model="ir.model.data">
            <value
              eval="[('module', '=', 'purchase'), ('name', '=', 'email_template_edi_purchase')]"
              />
        </function>
       <!-- remove noupdate -->
        <value eval="{'noupdate': False}" />
    </function>
    
    <record id="purchase.email_template_edi_purchase" model="mail.template">
        <field name="report_template" ref="request_for_quotation"/>
    </record>
    
    <function name="write" model="ir.model.data">
        <function name="search" model="ir.model.data">
            <value
              eval="[('module', '=', 'purchase'), ('name', '=', 'email_template_edi_purchase')]"
              />
        </function>
        <value eval="{'noupdate': True}" />
    </function>
</odoo>