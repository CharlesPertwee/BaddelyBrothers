<odoo>
    <data>
        <record model="ir.module.category" id="process_management">
          <field name="name">Process</field>
          <field name="description">User access level for process module</field>
          <field name="sequence">3</field>
        </record>
        
        <record id="group_process_user" model="res.groups">
         <field name="name">User</field>
         <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
         <field name="users" eval="[(4, ref('base.user_root'))]"/>
         <field name="category_id" ref="bb_process.process_management"/>
        </record>

        <record id="group_process_manager" model="res.groups">
          <field name="name">Manager</field>
          <field name="implied_ids" eval="[(4, ref('bb_process.group_process_user'))]"/>
          <field name="category_id" ref="bb_process.process_management"/>
        </record>
        
        
    </data>
</odoo>