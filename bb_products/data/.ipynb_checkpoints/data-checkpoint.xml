<odoo>
    <data noupdate="1">
        <record id="product_uom_sheets_bigger" model="uom.uom">
            <field name="category_id" ref="product_uom_categ_unit"/>
            <field name="name">1000 Sheets</field>
            <field name="uom_type">bigger</field>
            <field name="factor_inv" eval="1000.00"/>
            <field name="factor" eval="1000.00"/>
            <field name="rounding" eval="0.10000"/>
        </record>
        <record id="product_uom_sheets" model="uom.uom">
            <field name="category_id" ref="product_uom_categ_unit"/>
            <field name="name">Sheets</field>
            <field name="uom_type">smaller</field>
            <field name="factor_inv" eval="1.00000"/>
            <field name="factor" eval="1.00000"/>
            <field name="rounding" eval="0.001"/>
            <field name="rounding" eval="0.01000"/>
        </record>
    </data>
</odoo>
    