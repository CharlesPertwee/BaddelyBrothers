<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="address_label">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="web.basic_layout">
                        <div class="page" style="font-family: Georgia !important;">
                            <div class="container" style="font-size:14pt; padding:1px;">
                                <div>
                                    <p><br/>
                                    <span t-esc="o.name"/><br/>
                                    <t t-if="o.parent_id.name">
                                        <span t-esc="o.parent_id.name"/><br/>
                                    </t>
                                    <t t-if="o.street"><span t-field="o.street"/><br/></t>
                                    <t t-if="o.street2"><span t-field="o.street2"/><br/></t>
                                    <t t-if="o.city"><span t-field="o.city"/><br/></t>
                                    <t t-if="o.state_id"><span t-field="o.state_id"/><br/></t>
                                    <t t-if="o.zip"><span t-field="o.zip"/><br/></t></p>
                                </div>
                            </div>
                        </div>
                    </t>
                </t>
            </t>
        </template>
</odoo>
